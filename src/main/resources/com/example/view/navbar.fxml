	</JFXButton>
	<JFXButton fx:id="btnHelp" buttonType="RAISED" contentDisplay="TOP" onAction="#onAction" prefHeight="35.0" prefWidth="300.0" text="Help" textFill="WHITE" GridPane.rowIndex="4">
