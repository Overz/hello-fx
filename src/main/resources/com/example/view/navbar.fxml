<VBox alignment="TOP_CENTER" prefHeight="776.0" prefWidth="421.0" stylesheets="@../styles/navbar.css" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.controller.NavBarController">
  <GridPane fx:id="grid" alignment="TOP_CENTER" VBox.vgrow="ALWAYS">
	<columnConstraints>
		<ColumnConstraints halignment="CENTER" hgrow="ALWAYS" maxWidth="1.7976931348623157E308" />
	</columnConstraints>
	<rowConstraints>
		<RowConstraints maxHeight="150.0" minHeight="150.0" prefHeight="150.0" vgrow="ALWAYS" />
