		<RowConstraints maxHeight="1.7976931348623157E308" minHeight="35.0" prefHeight="35.0" valignment="BOTTOM" vgrow="ALWAYS" />
	</rowConstraints>
	<ImageView fitHeight="100.0" fitWidth="100.0" pickOnBounds="true" preserveRatio="true" GridPane.halignment="CENTER" GridPane.hgrow="ALWAYS" GridPane.valignment="CENTER" GridPane.vgrow="ALWAYS">
